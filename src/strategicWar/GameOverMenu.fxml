<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="victorLabel" alignment="CENTER" layoutX="163.0" layoutY="49.0" prefHeight="70.0" prefWidth="274.0" text="YOU LOSE!" textAlignment="CENTER">
         <font>
            <Font name="Avenir Next Heavy" size="48.0" />
         </font>
      </Label>
      <Button layoutX="244.0" layoutY="149.0" mnemonicParsing="false" onAction="#retryPressed" text="RETRY">
         <font>
            <Font name="Avenir Next Heavy" size="24.0" />
